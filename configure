    { printf "%s\n" "$as_me:${as_lineno-$LINENO}: result: $acx_elpa_ok ($FCFLAGS_ELPA $LIBS_ELPA)" >&5
printf "%s\n" "$acx_elpa_ok ($FCFLAGS_ELPA $LIBS_ELPA)" >&6; }

    if test x$acx_elpa_ok != xyes; then

      { printf "%s\n" "$as_me:${as_lineno-$LINENO}: WARNING: Could not find the elpa library with openMP support" >&5
printf "%s\n" "$as_me: WARNING: Could not find the elpa library with openMP support" >&2;}

      if test x"$test_elpa_without_openmp" = xTrue; then
        { printf "%s\n" "$as_me:${as_lineno-$LINENO}: WARNING: Trying to find elpa library without openMP support" >&5
printf "%s\n" "$as_me: WARNING: Trying to find elpa library without openMP support" >&2;}
      else
        FCFLAGS_ELPA=""
        LIBS_ELPA=""
      fi

    else


printf "%s\n" "#define HAVE_ELPA 1" >>confdefs.h

      # Elpa with openmp is found, no need to test elpa without openmp
      test_elpa_without_openmp=False

    fi
  fi

  # Section to test elpa without openMP support
  if test x$test_elpa_without_openmp = xTrue; then

    { printf "%s\n" "$as_me:${as_lineno-$LINENO}: checking for elpa without openMP support" >&5
printf %s "checking for elpa without openMP support... " >&6; }
      # Set LIBS_ELPA
      if test ! -z "$with_elpa_prefix"; then
        LIBS_ELPA="-L$with_elpa_prefix/lib -lelpa"
      else
        LIBS_ELPA="-lelpa"
      fi

    LIBS="$LIBS_ELPA $acx_elpa_save_LIBS $LIBS_LAPACK $LIBS_BLAS"
    cat > conftest.$ac_ext <<_ACEOF
