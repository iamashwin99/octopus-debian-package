    save_LDFLAGS=$LDFLAGS
    save_libdir=$libdir
    eval "libdir=/foo; wl=\"$lt_prog_compiler_wl_FC\"; \
	 LDFLAGS=\"\$LDFLAGS $hardcode_libdir_flag_spec_FC\""
