# -*- coding: utf-8 mode: shell-script -*-

Test       : Analytical Norm-conserving potential for Hydrogen
Program    : octopus
TestGroups : short-run, finite_systems_3d
Enabled    : Yes

Input: 42-full_potential_anc.01-gs.inp
match ; SCF convergence    ; GREPCOUNT(static/info, 'SCF converged') ; 1

match ;  Total energy        ; GREPFIELD(static/info, 'Total       =', 3) ; -0.50008286
match ;  Ion-ion energy      ; GREPFIELD(static/info, 'Ion-ion     =', 3) ;  0.00000000
match ;  Eigenvalues sum     ; GREPFIELD(static/info, 'Eigenvalues =', 3) ; -0.50008286
match ;  Hartree energy      ; GREPFIELD(static/info, 'Hartree     =', 3) ;  0.00000000
match ;  Kinetic energy      ; GREPFIELD(static/info, 'Kinetic     =', 3) ;  0.49910430
match ;  External energy     ; GREPFIELD(static/info, 'External    =', 3) ; -0.99918715

match ; Eigenvalue 1                  ; GREPFIELD(static/info, ' 1   -- ', 3) ; -0.500083
match ; Eigenvalue 2                  ; GREPFIELD(static/info, ' 2   -- ', 3) ; -0.104918
match ; Eigenvalue 3                  ; GREPFIELD(static/info, ' 3   -- ', 3) ; -0.085769

