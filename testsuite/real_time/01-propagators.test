# -*- coding: utf-8 mode: shell-script -*-

Test       : Propagators
Program    : octopus
TestGroups : short-run, real_time
Enabled    : Yes

#This test checks the time propagation, for various propagators

# ground state
Processors : 1
Input      : 01-propagators.01-gs.inp
Precision  : 2e-08
match ; SCF convergence ; GREPCOUNT(static/info, 'SCF converged') ; 1
Precision: 5.30e-07
match ;   Initial energy   ; GREPFIELD(static/info, 'Total       =', 3) ; -10.6068424

Processors : 4
Input      : 01-propagators.02-expmid.inp
Precision: 8.30e-15
match ;   Forces [step  1]   ; LINEFIELD(td.general/coordinates, -21, 15) ; 0.085374918107496
Precision: 4.18e-15
match ;   Forces [step 20]   ; LINEFIELD(td.general/coordinates, -1, 15) ; 0.0796669932158818
Precision: 5.30e-14
match ;  Energy [step  1]  ; LINEFIELD(td.general/energy, -21, 3) ; -10.6068424020538
Precision: 5.30e-13
match ;  Energy [step 20]  ; LINEFIELD(td.general/energy, -1, 3) ; -10.6063498271684
Precision: 4.67e-15
match ;  Multipoles [step  1]  ; LINEFIELD(td.general/multipoles, -21, 4) ; 2.28273040118846e-15
Precision: 6.33e-15
match ;  Multipoles [step 20]  ; LINEFIELD(td.general/multipoles, -1, 4) ; -0.126551382324362

