# -*- coding: utf-8 mode: shell-script -*-

Test       : DFT+U with species_full_delta
Program    : octopus
TestGroups : short-run, finite_systems_3d, lda_u
Enabled    : Yes

Processors : 4

Input      : 11-full_delta.01-gs.inp

match ; SCF convergence    ; GREPCOUNT(static/info, 'SCF converged') ; 1

Precision: 1.38e-07
match ;  Total energy        ; GREPFIELD(static/info, 'Total       =', 3) ; -2.7573712099999996
Precision: 1.04e-07
match ;  Eigenvalues sum     ; GREPFIELD(static/info, 'Eigenvalues =', 3) ; -2.07179991
Precision: 1.06e-07
match ;  Hartree energy      ; GREPFIELD(static/info, 'Hartree     =', 3) ; 2.12665618
Precision: 4.57e-06
match ;  Exchange energy     ; GREPFIELD(static/info, 'Exchange    =', 3) ; -0.9149045
Precision: 5.70e-08
match ;  Correlation energy  ; GREPFIELD(static/info, 'Correlation =', 3) ; -0.11407655
Precision: 1.49e-07
match ;  Kinetic energy      ; GREPFIELD(static/info, 'Kinetic     =', 3) ; 2.98738073
Precision: 3.44e-07
match ;  External energy     ; GREPFIELD(static/info, 'External    =', 3) ; -6.879744089999999
Precision: 1.87e-11
match ;  Hubbard energy      ; GREPFIELD(static/info, 'Hubbard     =', 3) ; 0.037315730000000005

Precision: 9.68e-08
match ;  Occupation 1s dn  ; LINEFIELD(static/occ_matrices, -1, 1) ; 1.93560239

Precision: 5.99e-06
match ;  U   ; LINEFIELD(static/effectiveU, -4, 4) ; 1.197473

Precision: 5.18e-04
match ;  Eigenvalue   [1]  ; GREPFIELD(static/info, '1   --', 3) ; -1.0359
Precision: 8.08e-06
match ;  Eigenvalue   [2]  ; GREPFIELD(static/info, '2   --', 3) ; 0.161508
